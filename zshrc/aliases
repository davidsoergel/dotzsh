
# Command Aliases
#alias vi=vim
alias ls-al='ls -alh'
alias la='ls -alh'
alias history='fc -l -f -i 1'
alias h='fc -l -f -i 1'
alias h='fc -l -f -i 1 | grep $1'
alias mkdir='nocorrect mkdir'

alias rmate='~/.zsh/rmate'

#alias fixssh='source $HOME/bin/fixssh'

alias dotzsh='~/.zsh/dotzsh'

# Global aliases
alias -g L='| less'